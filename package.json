{
  "name": "aft-ui",
  "version": "4.0.0",
  "description": "Automated Functional Testing (AFT) core package supporting UI testing via plugins",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && tsc --declaration",
    "pretest": "npm run build",
    "test": "./node_modules/.bin/jasmine JASMINE_CONFIG_PATH=./jasmine.json",
    "coverage": "nyc npm run test"
  },
  "keywords": [
    "aft",
    "automation",
    "e2e",
    "integration",
    "functional",
    "ui"
  ],
  "author": "Jason Holt Smith <bicarbon8@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "aft-core": "^4.0.0",
    "es6-promise": "^4.2.8"
  },
  "devDependencies": {
    "@types/jasmine": "^3.4.4",
    "@types/node": "^12.7.12",
    "jasmine": "^3.5.0",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "typescript": "^3.6.4"
  }
}
